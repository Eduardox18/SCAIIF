      </VBox>
      <JFXDrawer fx:id="menuDrawer" defaultDrawerSize="230.0" onMouseClicked="#mostrarIcono" prefHeight="500.0" prefWidth="700.0" />
      <JFXHamburger fx:id="menuIcon" layoutX="15.0" layoutY="15.0" />
   </children>
</AnchorPane>
