<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="DiaFestivo">
    <select id="getDiasFestivosbyPeriodo" parameterType="int" resultType="modelo.pojos.DiaFestivo">
        SELECT diaFestivo FROM diaFestivo WHERE idPeriodo = #{idPeriodo};
    </select>
</mapper>